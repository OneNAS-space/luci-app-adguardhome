config adguardhome 'config'
	option enabled '0'
	option user 'adguardhome'
	option group 'adguardhome'
	option verbose '0'
	option config_file '/etc/adguardhome/adguardhome.yaml'
	option work_dir '/var/lib/adguardhome'
	option bin_path '/usr/bin/AdGuardHome'
	option redirect 'none'
	option update_url 'https://static.adtidy.org/adguardhome/release/AdGuardHome_linux_${Arch}.tar.gz'
	option ucitracktest '2'
	list old_redirect 'dnsmasq-upstream'
	list old_port '1745'
	list old_enabled '0'
